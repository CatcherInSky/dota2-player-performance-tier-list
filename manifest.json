{
  "manifest_version": 1,
  "type": "WebApp",
  "permissions": [
    "Clipboard",
    "Hotkeys",
    "Window",
    "FileSystem",
    "GameInfo",
    "Web"
  ],
  "meta": {
    "name": "Player Tier List",
    "author": "CatcherInSky",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.235.0",
    "description": "Dota 2 Player Performance Analysis and Player Tier List",
    "dock_button_title": "Dota 2 Performance",
    "icon": "assets/1000.jpg",
    "icon_gray": "assets/1000.jpg",
    "launcher_icon": "assets/1000.jpg",
    "tray_icon": "assets/1000.jpg",
    "splash_image": "assets/1000.jpg",
    "window_icon": "assets/1000.jpg"
  },
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [7314]
    },
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "show_in_taskbar": false,
        "is_background_page": true,
        "open_dev_tools": false
      },
      "desktop": {
        "file": "desktop.html",
        "show_in_taskbar": true,
        "transparent": false,
        "resizable": true,
        "show_minimize": true,
        "clickthrough": false,
        "size": {
          "width": 1280,
          "height": 720
        },
        "min_size": {
          "width": 960,
          "height": 540
        },
        "max_size": {
          "width": 1920,
          "height": 1080
        },
        "start_position": {
          "top": 100,
          "left": 100
        },
        "mute": false,
        "open_dev_tools": false
      },
      "history": {
        "file": "history.html",
        "show_in_taskbar": true,
        "transparent": false,
        "resizable": true,
        "show_minimize": true,
        "clickthrough": false,
        "size": {
          "width": 960,
          "height": 540
        },
        "min_size": {
          "width": 480,
          "height": 270
        },
        "start_position": {
          "top": 100,
          "left": 100
        },
        "mute": false
      },
      "comment": {
        "file": "comment.html",
        "show_in_taskbar": true,
        "transparent": false,
        "resizable": true,
        "show_minimize": true,
        "clickthrough": false,
        "size": {
          "width": 960,
          "height": 540
        },
        "min_size": {
          "width": 480,
          "height": 270
        },
        "start_position": {
          "top": 100,
          "left": 100
        },
        "mute": false
      }
    },
    "force_browser": "overwolf",
    "game_events": [7314],
    
    "hotkeys": {
      "toggle_windows": {
        "title": "Toggle All Windows",
        "action-type": "custom",
        "default": "Alt+Shift+D"
      }
    },
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [7314]
        },
        "tracked": true,
        "start_minimized": false
      }
    ],
    "developer": {
      "enable_auto_refresh": true
    },
    "open_dev_tools": false
  }
}
