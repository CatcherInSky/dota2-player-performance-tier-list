{
  "manifest_version": 1.1,
  "type": "App",
  "meta": {
    "name": "Dota 2 Performance Tier List",
    "author": "Your Name",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.235.0",
    "description": "Dota 2 Player Performance Analysis and Tier List",
    "dock_button_title": "Dota 2 Performance",
    "icon": "assets/128.jpg",
    "icon_gray": "assets/128.jpg",
    "launcher_icon": "assets/128.jpg",
    "splash_image": "assets/728.jpg",
    "window_icon": "assets/32.jpg",
    "support_email": "support@example.com",
    "legal": {
      "terms_url": "https://example.com/terms",
      "privacy_policy_url": "https://example.com/privacy"
    }
  },
  "permissions": [
    "Hotkeys",
    "Window",
    "FileSystem",
    "GameInfo",
    "ExtensionsIO"
  ],
  "data": {
    "start_window": "background",
    "externally_connectable": {
      "matches": []
    },
    "windows": {
      "background": {
        "file": "background.html",
        "is_background_page": true,
        "transparent": false,
        "resizable": false,
        "allow_scripts_to_close_window": true,
        "show_in_taskbar": false,
        "mute": false,
        "disable_shadow": true,
        "size": {
          "width": 400,
          "height": 400
        }
      },
      "desktop": {
        "file": "desktop.html",
        "transparent": true,
        "clickthrough": false,
        "resizable": true,
        "show_in_taskbar": false,
        "mute": false,
        "disable_shadow": true,
        "show_minimize_button": true,
        "show_maximize_button": true,
        "show_close_button": true,
        "size": {
          "width": 1280,
          "height": 720
        },
        "min_size": {
          "width": 960,
          "height": 540
        },
        "max_size": {
          "width": 1920,
          "height": 1080
        },
        "start_position": {
          "top": 100,
          "left": 100
        }
      },
      "ingame": {
        "file": "ingame.html",
        "transparent": true,
        "resizable": true,
        "show_in_taskbar": false,
        "grab_focus": true,
        "clickthrough": false,
        "mute": false,
        "disable_shadow": true,
        "in_game_only": true,
        "show_minimize_button": true,
        "show_maximize_button": false,
        "show_close_button": false,
        "size": {
          "width": 960,
          "height": 540
        },
        "min_size": {
          "width": 480,
          "height": 270
        },
        "start_position": {
          "top": 100,
          "left": 100
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [7314]
    },
    "game_events": [7314],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [7314]
        },
        "start_minimized": false
      }
    ],
    "hotkeys": {
      "toggle_windows": {
        "title": "Toggle All Windows",
        "action-type": "custom",
        "default": "Alt+Shift+D"
      },
      "toggle_ingame": {
        "title": "Toggle Ingame Overlay",
        "action-type": "custom",
        "default": "Alt+Shift+F"
      }
    }
  }
}
